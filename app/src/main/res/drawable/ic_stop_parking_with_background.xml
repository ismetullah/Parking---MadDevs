<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape
                android:shape="oval">
            <solid android:color="@color/grey_100" />
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:right="0.5dp" android:left="0.5dp" android:bottom="1dp">
        <shape android:shape="oval">
            <size android:width="35dp" android:height="35dp"/>
            <solid android:color="@color/colorPrimaryDark"/>
        </shape>
    </item>
    <item android:drawable="@drawable/ic_stop_parking"
          android:bottom="@dimen/mini_margin"
          android:top="@dimen/mini_margin"
          android:left="@dimen/mini_margin"
          android:right="@dimen/mini_margin"/>
</layer-list>